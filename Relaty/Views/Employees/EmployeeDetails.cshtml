@model Relaty.ViewModels.EmployeeTitlesModel
@{
    ViewData["Title"] = "EmployeeDetails";
}

<br />

<div class="form-group col-3">
    <h2>@Model.Employee.Name</h2>
</div>
<div class="form-group col-3">
    <h5>Email: @Model.Employee.Email</h5>
</div>
<div class="form-group col-3">
    <h5>Title: @Model.Employee.Title.Name</h5>
</div>
<div class="form-group col-4 ">
    <h5>Projects:</h5>
    @foreach (var employeProject in Model.Employee.ProjectsEmployees)
    {
        <span class="btn btn-primary">@employeProject.Project.Name</span>
    }
</div>

